{
  "name": "react-tutorial",
  "version": "1.0.0",
  "description": "Follow the tutorial of React to learn React :3",
  "main": "app.js",
  "scripts": {
    "build:w": "babel src --watch --out-dir js",
    "build": "babel src --out-dir js",
    "debug": "concurrent \"npm run build:w\" \"node .\" ",
    "minify": "minify js/",
    "clean": "rm -rf js/",
    "start": "npm run clean && npm run build && npm run minify && node ."
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hckhanh/react-tutorial.git"
  },
  "keywords": [
    "react",
    "facebook",
    "learn",
    "demo"
  ],
  "author": "hckhanh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hckhanh/react-tutorial/issues"
  },
  "homepage": "https://github.com/hckhanh/react-tutorial#readme",
  "dependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "body-parser": "^1.14.2",
    "express": "^4.13.4",
    "install": "^0.4.1",
    "jquery": "^2.2.0",
    "minifier": "^0.7.1",
    "node-minify": "^1.3.7",
    "npm": "^3.6.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "concurrently": "^1.0.0"
  }
}
